{
  "name": "prism-nsis",
  "version": "0.4.0",
  "description": "NSIS support for the PrismJS syntax highlighter",
  "repository": "https://github.com/idleberg/prismjs-nsis",
  "license": "MIT",
  "scripts": {
    "build": "rollup --config",
    "dev": "npm run start",
    "fix": "eslint --fix ./src",
    "lint": "eslint ./src",
    "start": "rollup --watch --config",
    "test": "npm run lint"
  },
  "keywords": [
    "highlighter",
    "nsis",
    "prism",
    "prism.js",
    "syntax highlighter"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "eslint-config-recommended": "^4.1.0",
    "rollup": "^2.24.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "dependencies": {
    "husky": "^4.2.5"
  }
}
