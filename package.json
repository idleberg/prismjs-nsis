{
  "name": "prism-nsis",
  "version": "0.8.2",
  "description": "NSIS support for the PrismJS syntax highlighter",
  "repository": "https://github.com/idleberg/prismjs-nsis",
  "license": "MIT",
  "main": "dist/nsis.js",
  "files:": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rollup --config",
    "dev": "npm run start",
    "fix": "eslint --fix ./src --ignore-path .gitignore",
    "lint": "eslint ./src --ignore-path .gitignore",
    "start": "rollup --watch --config",
    "test": "npm run lint"
  },
  "keywords": [
    "highlighter",
    "nsis",
    "prism",
    "prism.js",
    "syntax highlighter"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-config-recommended": "^4.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "retrie": "^0.1.1",
    "rollup": "^2.77.0",
    "rollup-plugin-entry-code-injector": "^1.0.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "lint-staged": {
    "*.(js|json)": "eslint --cache --fix"
  }
}
